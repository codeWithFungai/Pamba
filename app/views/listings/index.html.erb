<h1>Listings</h1>

<% @listings.each do |listing| %>
  <ul>
    <li><%= listing.title %></li>
    <li><%= listing.address %></li>
    <li><%= listing.city %></li>
    <li><%= listing.description %></li>
    <li><%= listing.price %></li>
    <li><%= listing.kitchen %></li>
    <li><%= listing.private_bathroom %></li>
    <li><%= listing.laundry %></li>
    <li><%= listing.distance %></li>
    <li><%= listing.style %></li>
    <li><%= listing.nr_of_rooms %></li>
    <li><%= listing.image %></li>
  </ul>
<% end %>
